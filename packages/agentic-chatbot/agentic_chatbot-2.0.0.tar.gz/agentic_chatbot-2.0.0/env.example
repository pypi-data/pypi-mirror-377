# AI Agents Chatbot Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Google Search API Configuration (for most accurate search results)
# Get your API key from: https://console.cloud.google.com/
# Enable Custom Search API and create credentials
GOOGLE_API_KEY=your_google_api_key_here

# Google Custom Search Engine ID
# Set up at: https://cse.google.com/cse/
GOOGLE_CSE_ID=your_google_cse_id_here

# Optional: Custom OpenAI API Base URL (for Azure OpenAI or other providers)
# OPENAI_API_BASE=https://api.openai.com/v1

# Optional: Security Agent Configuration
SECURITY_THREAT_THRESHOLD=0.7
SECURITY_MODEL=gpt-4o

# Optional: Context Agent Configuration
CONTEXT_MODEL=gpt-3.5-turbo
CONTEXT_RELEVANCE_THRESHOLD=0.4

# Optional: Model Selection Agent Configuration
MODEL_SELECTION_COST_SENSITIVITY=medium
MODEL_SELECTION_PERFORMANCE_PREFERENCE=balanced

# Optional: Logging Configuration
LOG_LEVEL=INFO
DEBUG=false

# Optional: Demo Configuration
DEMO_ENABLED=true
DEMO_PORT=8501

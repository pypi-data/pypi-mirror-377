{
  "id": "web-application",
  "name": "Web Application",
  "description": "Base template for web applications with common setup and best practices",
  "version": "1.0.0",
  "project_type": "web-app",
  "technology_stack": [
    "html",
    "css",
    "javascript",
    "node.js",

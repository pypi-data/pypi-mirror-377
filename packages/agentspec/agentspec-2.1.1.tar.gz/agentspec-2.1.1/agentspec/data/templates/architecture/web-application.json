  "default_tags": [
    "frontend",
    "web",
    "performance",
    "accessibility",
    "security",
    "testing",
    "code-quality"
  ],
  "required_instructions": [
    "project_structure",
    "build_system",
    "testing_framework",
    "code_quality_tools"
  ],
  "optional_instructions": [
    "pwa_configuration",
    "seo_optimization",
    "analytics_integration"

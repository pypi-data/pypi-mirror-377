  "parameters": {
    "build_tool": {
      "type": "string",
      "default": "vite",
      "description": "Build tool to use",
      "required": false,
      "options": ["vite", "webpack", "parcel", "rollup"]
    },
    "package_manager": {
      "type": "string",
      "default": "npm",
      "description": "Package manager to use",
      "required": false,
      "options": ["npm", "yarn", "pnpm"]
    },
    "linting": {
      "type": "boolean",
      "default": true,
      "description": "Enable code linting",
      "required": false
    },
    "formatting": {
      "type": "boolean",
      "default": true,
      "description": "Enable code formatting",
      "required": false
    }
  },
  "conditions": [
    {
      "type": "file_exists",
      "value": "package.json",
      "operator": "exists",
      "weight": 0.8
    },
    {
      "type": "file_exists",
      "value": "index.html",
      "operator": "exists",
      "weight": 0.6
    }
  ],
  "metadata": {
    "category": "frontend",

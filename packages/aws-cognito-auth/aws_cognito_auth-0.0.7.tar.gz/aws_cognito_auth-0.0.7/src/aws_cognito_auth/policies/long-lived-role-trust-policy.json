{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "{lambda_user_arn}"
      },
      "Action": "sts:AssumeRole",
      "Condition": {
        "StringEquals": {
          "aws:RequestedRegion": [
            "ap-southeast-1",
            "us-east-1",
            "us-west-2"
          ]
        }
      }
    }
  ]
}

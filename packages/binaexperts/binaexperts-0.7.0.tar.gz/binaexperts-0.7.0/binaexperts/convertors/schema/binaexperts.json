{
  "type": "object",
  "properties": {
    "info": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "organization": {
          "type": "string"
        },
        "dataset": {
          "type": "string"
        },
        "dataset_type": {
          "type": "string"
        },
        "date_created": {
          "type": "string",
          "format": "date"
        }
      },
      "required": [
        "description",
        "organization",
        "dataset",
        "dataset_type",
        "date_created"
      ]
    },
    "images": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "file_name": {
            "type": "string"
          },
          "width": {
            "type": "integer"
          },
          "height": {
            "type": "integer"
          }
        },
        "required": [
          "id",
          "file_name",
          "width",
          "height"
        ]
      }
    },
    "annotations": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "image_id": {
            "type": "integer"
          },
          "category_id": {
            "type": "integer"
          },
          "bbox": {
            "type": "array",
            "items": {
              "type": "number"
            },
            "minItems": 4,
            "maxItems": 4
          },
          "segmentation": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "number"
              }
            }
          },
          "area": {
            "type": "number"
          },
          "iscrowd": {
            "type": "integer"
          }
        },
        "required": [
          "id",
          "image_id",
          "category_id",
          "bbox"
        ]
      }
    },
    "categories": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "supercategory": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "name"
        ]
      }
    },
    "licenses": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "url": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "name",
          "url"
        ]
      }
    },
    "labels": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "label_id": {
            "type": "integer"
          },
          "label_name": {
            "type": "string"
          }
        }
      }
    },
    "classifications": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "classification_id": {
            "type": "integer"
          },
          "classification_name": {
            "type": "string"
          }
        }
      }
    },
    "augmentation_settings": {
      "type": "object",
      "properties": {
        "augmentation_type": {
          "type": "string"
        },
        "parameters": {
          "type": "object"
        }
      }
    },
    "tile_settings": {
      "type": "object",
      "properties": {
        "tile_type": {
          "type": "string"
        },
        "parameters": {
          "type": "object"
        }
      }
    },
    "false_positive": {
      "type": "object",
      "properties": {
        "fp_type": {
          "type": "string"
        },
        "details": {
          "type": "object"
        }
      }
    },
    "errors": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "annotation_type": {
            "type": "string"
          },
          "annotation": {
            "type": "object",
            "properties": {
              "x": {
                "type": "number"
              },
              "y": {
                "type": "number"
              },
              "width": {
                "type": "number"
              },
              "height": {
                "type": "number"
              }
            }
          },
          "image_id": {
            "type": "integer"
          },
          "image_file_name": {
            "type": "string"
          },
          "error_message": {
            "type": "string"
          }
        },
        "required": [
          "annotation_type",
          "annotation",
          "image_id",
          "image_file_name",
          "error_message"
        ]
      }
    }
  }
}
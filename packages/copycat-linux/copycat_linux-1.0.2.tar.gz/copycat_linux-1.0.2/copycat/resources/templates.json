    {
      "name": "json-schema",
      "description": "Basic JSON schema template",
      "category": "development",
      "content": "{\n  \"type\": \"object\",\n  \"properties\": {\n    \"{{PROPERTY_NAME}}\": {\n      \"type\": \"{{PROPERTY_TYPE}}\",\n      \"description\": \"{{PROPERTY_DESC}}\"\n    }\n  },\n  \"required\": [\"{{PROPERTY_NAME}}\"]\n}",
      "placeholders": [
        {
          "name": "PROPERTY_NAME",
          "description": "Property name",
          "type": "text",
          "required": true,
          "default": "name"
        },
        {
          "name": "PROPERTY_TYPE",
          "description": "Property type",
          "type": "select",
          "options": ["string", "number", "boolean", "object", "array"],
          "default": "string"
        },
        {
          "name": "PROPERTY_DESC",
          "description": "Property description",
          "type": "text",
          "required": false,
          "default": ""
        }
      ],
      "usage_count": 0,
      "created": "2025-09-15",
      "tags": ["json", "schema", "development"]
    },
    {
      "name": "mcp-server-config",
      "description": "MCP server configuration template",
      "category": "development",
      "content": "{\n  \"mcpServers\": {\n    \"{{SERVER_NAME}}\": {\n      \"command\": \"{{COMMAND}}\",\n      \"args\": [{{ARGS}}],\n      \"env\": {\n        \"{{ENV_KEY}}\": \"{{ENV_VALUE}}\"\n      }\n    }\n  }\n}",
      "placeholders": [
        {
          "name": "SERVER_NAME",
          "description": "MCP server name",
          "type": "text",
          "required": true
        },
        {
          "name": "COMMAND",
          "description": "Command to run the server",
          "type": "text",
          "required": true,
          "default": "npx"
        },
        {
          "name": "ARGS",
          "description": "Command arguments (comma-separated, quoted)",
          "type": "text",
          "required": true
        },
        {
          "name": "ENV_KEY",
          "description": "Environment variable key",
          "type": "text",
          "required": false
        },
        {
          "name": "ENV_VALUE",
          "description": "Environment variable value",
          "type": "sensitive",
          "required": false
        }
      ],
      "usage_count": 0,
      "created": "2025-09-15",
      "tags": ["mcp", "server", "configuration"]
    },

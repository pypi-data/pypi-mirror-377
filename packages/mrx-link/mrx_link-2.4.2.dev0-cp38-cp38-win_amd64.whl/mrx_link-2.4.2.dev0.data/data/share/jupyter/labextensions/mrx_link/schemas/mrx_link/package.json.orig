{
  "name": "mrx_link",
  "version": "2.4.2-dev.0",
  "description": "A simple AI/ML modeling tool, Linkâ„¢ ensures a smoother flow and a better experience throughout the model development cycle.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://link.makinarocks.ai",
  "bugs": {
    "url": "https://github.com/makinarocks/makina-link-release/issues"
  },
  "license": "UNLICENSED",
  "private": true,
  "author": {
    "name": "makinarocks",
    "email": "devops@makinarocks.ai"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,.js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/css/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/makinarocks/makina-link.git"
  },
  "scripts": {
    "build": "genversion --source ./genversion/dev --es6 --semi src/logger/version.ts && npm run build:lib && npm run build:labextension",
    "build:prod": "npm run clean:all && genversion --source ./genversion/prod --es6 --semi src/logger/version.ts && npm run build:lib && npm run build:labextension:prod",
    "build:labextension": "jupyter labextension build --development True .",
    "build:labextension:prod": "jupyter labextension build --development False .",
    "build:lib": "tsc",
    "clean": "npm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf mrx_link/labextension",
    "clean:all": "npm run clean:lib && npm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "eslint:fix": "eslint --fix --ext .ts,.tsx .",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch .",
    "test": "jest",
    "test:unit": "jest --testPathPattern=src/__tests__/unit",
    "bump-dev-version": "bash ./scripts/bump_dev_version.sh"
  },
  "dependencies": {
    "@dagrejs/graphlib": "^2.2.1",
    "@emotion/react": "^11.11.5",
    "@emotion/styled": "^11.11.5",
    "@jupyterlab/application": "^4.2.0",
    "@jupyterlab/apputils": "^4.2.0",
    "@jupyterlab/cells": "^4.2.0",
    "@jupyterlab/coreutils": "^6.2.0",
    "@jupyterlab/docmanager": "^4.2.0",
    "@jupyterlab/filebrowser": "^4.2.0",
    "@jupyterlab/launcher": "^4.2.0",
    "@jupyterlab/mainmenu": "^4.2.0",
    "@jupyterlab/notebook": "^4.2.0",
    "@jupyterlab/services": "^7.2.0",
    "@mui/material": "^5.15.15",
    "@preact-signals/safe-react": "^0.6.1",
    "@preact/signals": "^1.2.3",
    "axios": "^1.7.2",
    "d3": "^7.9.0",
    "dagre": "^0.8.5",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "mobx": "^6.12.3",
    "mobx-react-lite": "^3.4.3",
    "plotly.js-dist-min": "^2.30.1",
    "react-colorful": "^5.6.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@jupyterlab/builder": "^4.2.0",
    "@preact/signals-react-transform": "^0.3.1",
    "@types/d3": "^5.16.7",
    "@types/dagre": "^0.7.52",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.0",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@types/react": "18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/react-svg-inline": "^2.1.6",
    "@types/toastr": "^2.1.43",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "genversion": "^3.2.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "typescript": "^5.4.4"
  },
  "sideEffects": [
    "style/**/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "mrx_link"
        }
      }
    },
    "extension": true,
    "outputDir": "mrx_link/labextension",
    "schemaDir": "schema"
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install \"Cython>=3.0\" \"jupyter-packaging>=0.10\" \"jupyterlab>=3.6,<3.7\"",
        "npm install"
      ]
    }
  }
}

uwsgi:
  http: :{{ server.port }}
  wsgi-file: src/core/server.py
{% for item in all_models %}

  # {{ item.model.name.lower_case }}
  route: ^/{{ item.module.name.kebab_case }}/{{ item.model.name.kebab_case }}/create static:../browser1/srv/{{ item.module.name.kebab_case }}/{{ item.model.name.kebab_case }}/index.html
  route: ^/{{ item.module.name.kebab_case }}/{{ item.model.name.kebab_case }}/index(.*) static:../browser1/srv/{{ item.module.name.kebab_case }}/{{ item.model.name.kebab_case }}/index$1
  load-file-in-cache: ../browser1/srv/{{ item.module.name.kebab_case }}/{{ item.model.name.kebab_case }}/index.html
  route: ^/{{ item.module.name.kebab_case }}/{{ item.model.name.kebab_case }}/(.*) static:../browser1/srv/{{ item.module.name.kebab_case }}/{{ item.model.name.kebab_case }}/instance.html
  load-file-in-cache: ../browser1/srv/{{ item.module.name.kebab_case }}/{{ item.model.name.kebab_case }}/instance.html

{% endfor %}


  static-map: /=../browser1/srv/
  static-index: index.html
  static-expires: /* 0
  
  py-auto-reload: 1
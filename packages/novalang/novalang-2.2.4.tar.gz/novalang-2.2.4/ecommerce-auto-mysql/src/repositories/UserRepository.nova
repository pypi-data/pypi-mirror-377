class UserRepository {
    function save(userData) {
        print "🏪 UserRepository: Saving user to MySQL database";
        print "📊 INSERT INTO users (email, name) VALUES...";
        print "✅ Database operation successful";
        return "User saved to database";
    }
    
    function findByEmail(email) {
        print "🏪 UserRepository: Finding user by email: " + email;
        print "📊 SELECT * FROM users WHERE email = " + email;
        return "User found in database";
    }
    
    function findAll() {
        print "🏪 UserRepository: Getting all users from database";
        print "📊 SELECT * FROM users";
        return "All users retrieved";
    }
}

class UserService {
    function createUser(email, firstName, lastName) {
        print "💼 UserService: Creating user: " + email;
        print "📝 Validating user data...";
        print "💾 Saving to database...";
        return "User saved successfully";
    }
    
    function getAllUsers() {
        print "💼 UserService: Getting all users";
        print "📊 Found 3 users in database";
        return "Users retrieved";
    }
    
    function deleteUser(id) {
        print "💼 UserService: Deleting user ID: " + id;
        print "🗑️ User deleted successfully";
        return "User deleted";
    }
}

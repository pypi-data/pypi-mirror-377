// NovaLang Test Example
@TestSuite
class UserServiceTest {
    
    @MockBean
    private repository: UserRepository;
    
    @InjectMock
    private service: UserService;
    
    @Test
    function testUserRecommendations(): void {
        let user = new User("test@example.com");
        let recommendations = service.getUserRecommendations(user, 5);
        assert(recommendations.length > 0);
    }
    
    @Test
    function testProcessUsersPerformance(): void {
        service.processUsers();
    }
}

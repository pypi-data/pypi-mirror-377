{
  "name": "novalang",
  "displayName": "NovaLang",
  "description": "Language support for NovaLang - The Full-Stack Programming Language",
  "version": "1.0.0",
  "publisher": "martinmaboya",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "keywords": [
    "novalang",
    "nova",
    "full-stack",
    "enterprise"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "novalang",
        "aliases": [
          "NovaLang",
          "nova"
        ],
        "extensions": [
          ".nova"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  }
}
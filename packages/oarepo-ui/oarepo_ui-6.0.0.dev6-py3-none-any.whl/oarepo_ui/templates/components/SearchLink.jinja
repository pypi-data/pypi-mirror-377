{#def className=None, search_link,  searchFacet, data, hasTooltip=True #}
{% set data_dict = data %}
{% set href = search_link ~ "?q=&f=" ~ searchFacet ~ ":" ~ data_dict.value %}

{% set url_data = {
    "href": href,
    "title": data_dict.title,
    "label": data_dict.label
} %}

<URL className={{className}} data={{url_data}} hasTooltip={{hasTooltip}}></URL>
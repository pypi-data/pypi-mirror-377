<data>
<record id="stock_picking_2_assigned" model="base.automation">
    <field name="action_server_id" ref="stock_picking_action_assigned" />
    <field name="trigger">on_write</field>
    <field name="filter_pre_domain">[('state','!=','assigned')]</field>
    <field name="filter_domain">[('state','=','assigned')]</field>
</record>

<record id="stock_picking_2_draft" model="base.automation">
    <field name="action_server_id" ref="stock_picking_action_draft" />
    <field name="trigger">on_write</field>
    <field name="filter_pre_domain">[('state','=','draft')]</field>
    <field name="filter_domain">[('state','=','draft')]</field>
</record>
</data>
</odoo>

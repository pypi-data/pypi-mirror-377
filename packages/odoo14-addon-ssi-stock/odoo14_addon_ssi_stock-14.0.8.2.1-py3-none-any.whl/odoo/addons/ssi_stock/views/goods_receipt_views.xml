<odoo>
<record id="goods_receipt_action" model="ir.actions.act_window">
    <field name="name">Goods Receipts</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">stock.picking</field>
    <field name="view_mode">tree,form</field>
    <field
            name="domain"
            eval="[('picking_type_category_id','=',ref('ssi_stock.picking_category_gr'))]"
        />
    <field
            name="context"
            eval="{'default_picking_type_category_id':ref('ssi_stock.picking_category_gr')}"
        />
</record>

<menuitem
        id="goods_receipt_menu"
        name="Goods Receipts"
        parent="menu_incoming_movement"
        groups="goods_receipt_viewer_group"
        action="goods_receipt_action"
        sequence="107"
    />
</odoo>

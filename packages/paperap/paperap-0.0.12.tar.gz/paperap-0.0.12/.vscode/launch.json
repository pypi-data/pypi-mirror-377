{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Unit Tests",
			"type": "debugpy",
			"request": "launch",
			"module": "unittest",
			"args": [
				"discover",
				"-s",
				"tests/unit",
				"--failfast",
			],
			"cwd": "${workspaceFolder}",
			"env": {
				"PYTHONPATH": "${workspaceFolder}/src"
			},
		}
	]
}
WS: /[ \x00\t]+/
NEWLINE: /\r?\n/
COMMENT: /;[^\r\n]*/

DIGIT: "0".."9"
INT: DIGIT+
DECIMAL: INT "." INT?
       | "." INT
SIGNED_INT: SIGN? INT
SIGN: "+"
    | "-"

EXP: ("E" | "e") SIGNED_INT
FLOAT: INT EXP
     | DECIMAL EXP?
SIGNED_FLOAT: SIGN? FLOAT

NUMBER: FLOAT | INT
SIGNED_NUMBER: SIGN? NUMBER
NUMERIC: NUMBER | SIGNED_NUMBER

SYMBOL: /[_A-Za-z]\w+/

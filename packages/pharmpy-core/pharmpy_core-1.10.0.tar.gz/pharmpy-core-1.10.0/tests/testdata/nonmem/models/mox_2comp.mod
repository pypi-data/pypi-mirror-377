$PROBLEM    MOXONIDINE PK,FINAL ESTIMATES, simulated data
$INPUT      ID VISI XAT2=DROP DGRP DOSE FLAG=DROP ONO=DROP
            XIME=DROP NEUY SCR AGE SEX NYH=DROP WT DROP ACE
            DIG DIU NUMB=DROP TAD TIME VIDD=DROP CRCL AMT SS II DROP
            CMT=DROP CONO=DROP DV EVID=DROP OVID=DROP
$DATA       mox_simulated_log.csv IGNORE=@
$SUBROUTINE ADVAN3 TRANS1

$PK
K12 = THETA(1)*EXP(ETA(1))
K21 = THETA(2)*EXP(ETA(2))
K   = THETA(3)*EXP(ETA(3))

$ERROR
IPRED = LOG(.025)
IF(F.GT.0) IPRED = LOG(F)
Y = IPRED+ERR(1)

$THETA  (0,2) ; POP_K12
$THETA  (0,20) ; POP_K21
$THETA  (0,4.5) ; POP_K
$OMEGA
0.0750
0.0564
0.0564
$SIGMA  0.109

$ESTIMATION METHOD=1 MAXEVAL=9999
$COV PRECOND=1 UNCONDITIONAL

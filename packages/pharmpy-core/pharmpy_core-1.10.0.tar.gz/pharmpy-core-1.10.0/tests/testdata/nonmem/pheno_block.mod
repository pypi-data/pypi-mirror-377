$PROBLEM PHENOBARB SIMPLE MODEL
$DATA pheno.dta IGNORE=@
$INPUT ID TIME AMT WGT APGR DV FA1
$SUBROUTINE ADVAN1 TRANS2
$ABBR REPLACE ETA_CL=ETA(1)
$ABBR REPLACE ETA_V=ETA(2)
$ABBR REPLACE ETA_S1=ETA(3)
$ABBR REPLACE ETA_MAT=ETA(4)
$ABBR REPLACE ETA_Q=ETA(5)
$PK
CL=THETA(1)*EXP(ETA_CL)
V=THETA(2)*EXP(ETA_V)
S1=V+ETA_S1
MAT=THETA(3)*EXP(ETA_MAT)
Q=THETA(4)*EXP(ETA_Q)

$ERROR
Y=F+F*EPS(1)

$THETA (0,0.00469307) ; TVCL
$THETA (0,1.00916) ; TVV
$THETA (0,0.1)
$THETA (0,0.1)
$OMEGA DIAGONAL(2)
0.0309626  ; IVCL
0.031128  ; IVV
$OMEGA 0.1
$OMEGA BLOCK(2)
0.0309626
0.0005 0.031128
$SIGMA 0.013241

$ESTIMATION METHOD=1 INTERACTION

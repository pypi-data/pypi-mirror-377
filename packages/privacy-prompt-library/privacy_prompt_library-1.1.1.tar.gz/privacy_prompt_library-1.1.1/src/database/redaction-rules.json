{
  "Physical Disabilities": {
    "Mobility Impairments": [
      {
        "id": "mobility_001",
        "pattern": "\\b(I am|I'm)\\s+(a\\s+)?wheelchair user\\b",
        "replacement": "$1 disability{redacted}",
        "flags": "gi",
        "contextual": true,
        "priority": "high"
      },
      {
        "id": "mobility_002", 
        "pattern": "\\b(I am|I'm)\\s+(paralyz(ed|is)|paralys(ed|is))\\b",
        "replacement": "$1 disability{redacted}",
        "flags": "gi",
        "contextual": true,
        "priority": "high"
      },
      {

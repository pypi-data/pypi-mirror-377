        "priority": "high"
      },
      {
        "id": "mobility_004",
        "pattern": "\\bmy wheelchair\\b",
        "replacement": "my disability{redacted}",
        "flags": "gi",
        "priority": "medium"
      },
      {
        "id": "mobility_005",
        "pattern": "\\bbecause of my paralysis\\b",
        "replacement": "due to my disability{redacted}",
        "flags": "gi",
        "priority": "medium"
      },
      {
        "id": "mobility_006",

{
  "Physical Disabilities": {
    "Mobility Impairments": [
      {
        "id": "mobility_001",
        "pattern": "\\b(I am|I'm)\\s+(a\\s+)?wheelchair user\\b",
        "replacement": "$1 disability{redacted}",
        "flags": "gi",
        "contextual": true,
        "priority": "high"
      },
      {
        "id": "mobility_002", 
        "pattern": "\\b(I am|I'm)\\s+(paralyz(ed|is)|paralys(ed|is))\\b",
        "replacement": "$1 disability{redacted}",
        "flags": "gi",
        "contextual": true,
        "priority": "high"
      },
      {
        "id": "mobility_003",
        "pattern": "\\b(I am|I'm)\\s+(paraplegic|quadriplegic)\\b", 
        "replacement": "$1 disability{redacted}",
        "flags": "gi",
        "contextual": true,
        "priority": "high"
      },
      {
        "id": "mobility_004",
        "pattern": "\\bmy wheelchair\\b",
        "replacement": "my disability{redacted}",
        "flags": "gi",
        "priority": "medium"
      },
      {
        "id": "mobility_005",
        "pattern": "\\bbecause of my paralysis\\b",
        "replacement": "due to my disability{redacted}",
        "flags": "gi",
        "priority": "medium"
      },
      {
        "id": "mobility_006",
        "pattern": "\\bas a wheelchair user\\b",
        "replacement": "as someone with disability{redacted}", 
        "flags": "gi",
        "priority": "medium"
      }
    ],
    "Chronic Physical": [
      {
        "id": "chronic_001",
        "pattern": "\\b(I have|I've got)\\s+(arthritis|muscular dystrophy|multiple sclerosis)\\b",
        "replacement": "$1 disability{redacted}",
        "flags": "gi",
        "priority": "high"
      },
      {
        "id": "chronic_002", 
        "pattern": "\\bmy (arthritis|fibromyalgia|lupus|ms)\\b",
        "replacement": "my disability{redacted}",
        "flags": "gi",
        "priority": "high"
      },
      {
        "id": "chronic_003",
        "pattern": "\\bbecause of my (chronic pain|arthritis|fibromyalgia)\\b",
        "replacement": "due to my disability{redacted}",
        "flags": "gi",
        "priority": "medium"
      }
    ]
  },

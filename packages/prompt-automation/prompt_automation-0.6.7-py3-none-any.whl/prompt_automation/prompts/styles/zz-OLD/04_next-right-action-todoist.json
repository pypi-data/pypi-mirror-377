{
  "schema": 1,
  "id": 51,
  "title": "Next Right Action Todoist",
  "style": "Reviews",
  "placeholders": [
    {
      "name": "next_action",
      "label": "\nNext Action: (I will...)",
      "multiline": false,
      "default": "",
      "remove_if_empty": [
        "I will",
        "Next Action",
        "- Next Action"
      ]
    },
    {
      "name": "so_that",
      "label": "\nOutcome to Achieve: (So That...)",
      "multiline": false,
      "default": "",
      "remove_if_empty": [
        "So That",
        "- So That",
        "so that I can"
      ]
    },
    {
      "name": "time_estimate",
      "label": "Time Estimate (e.g. 30m, 2h, 1 pomodoro)",
      "multiline": false,
      "default": "",
      "remove_if_empty": [
        "It will take",
        "Time Estimate",
        "- Time Estimate"
      ]
    }
  ],
  "template": [
    "I will {{next_action}} so that {{so_that}}. \u23f3{{time_estimate}}."
  ],
  "metadata": {
    "path": "Reviews/next-right-action-todoist.json",
    "tags": [
      "todoist",

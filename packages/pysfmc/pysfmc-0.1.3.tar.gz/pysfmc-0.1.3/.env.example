# Salesforce Marketing Cloud API Configuration
# Copy this file to .env and fill in your actual values

# Required SFMC Authentication Settings
SFMC_CLIENT_ID=your_client_id_here
SFMC_CLIENT_SECRET=your_client_secret_here
SFMC_ACCOUNT_ID=your_account_id_here
SFMC_SUBDOMAIN=your_subdomain_here

# Optional Client Configuration
# HTTP timeout in seconds (default: 30.0)
SFMC_CLIENT__TIMEOUT=30.0

# Rate limiting settings (default: 2500 requests per minute)
SFMC_RATE_LIMIT__REQUESTS_PER_MINUTE=2500
SFMC_RATE_LIMIT__BURST_LIMIT=100

# Debug and logging settings (default: false/INFO)
SFMC_DEBUG=false
SFMC_LOG_REQUESTS=false
SFMC_LOG_RESPONSES=false
SFMC_LOG_LEVEL=INFO

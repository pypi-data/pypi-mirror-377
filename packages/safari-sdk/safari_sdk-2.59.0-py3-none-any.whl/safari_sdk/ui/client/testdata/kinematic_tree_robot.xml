<mujoco>
  <compiler angle="degree" eulerseq="XYZ"/>
  <asset>
    <mesh name="mesh_left_arm" file="mesh_left_arm.stl"/>
    <mesh name="mesh_right_arm" file="mesh_right_arm.stl"/>
  </asset>
  <asset>
    <mesh name="mesh_torso" file="mesh_torso.stl"/>
    <mesh name="mesh_head" file="mesh_head.stl"/>
  </asset>
  <worldbody>
    <body name="" pos="1 2 3">
      <geom type="mesh" mesh="mesh_torso" pos="0.1 0.2 0.3" euler="10 20 30"/>
      <body name="right_arm" pos = "-1 -2 -3" euler="-180 0 0">
        <geom type="mesh" mesh="mesh_right_arm" pos="0 0 0" euler="0 0 0"/>
        <joint name="joint_right_arm" type="hinge" pos="-0.1 -0.2 -0.3" axis="0 0 1" limited="true" range="0 180"/>
      </body>
      <body name="left_arm" pos="1 -2 -3" quat="0 1 0 0">
        <geom type="mesh" mesh="mesh_left_arm" pos="0 0 0" euler="0 0 0"/>
        <joint name="joint_left_arm" type="hinge" axis="0 1 0" limited="true" range="-180 0"/>
        <site name="left_site_a" pos="0.01 0.02 0.03" euler="1 2 3"/>
        <site name="left_site_b" pos="-0.01 0.02 0.03" euler="-1 2 3"/>
      </body>
      <body name="head" pos="0 0 0">
        <geom type="mesh" mesh="mesh_head" pos="0 0 0" euler="60 0 0"/>
        <joint name="joint_head" type="hinge" range="-360 360"/>
        <site name="head_site"/>
      </body>
      <body>
        <geom type="mesh" mesh="mesh_left_arm"/>
        <joint type="hinge" range="-360 360"/>
        <site/>
      </body>
    </body>
  </worldbody>
</mujoco>

{
  "name": "ssb_dash_components",
  "version": "0.0.1",
  "description": "Dash Component library for SSB",
  "repository": {
    "type": "git",
    "url": "git://github.com/statisticsnorway/ssb-dash-components.git"
  },
  "bugs": {
    "url": "https://github.com/statisticsnorway/ssb-dash-components/issues"
  },
  "homepage": "https://github.com/statisticsnorway/ssb-dash-components",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:backends": "dash-generate-components ./src/lib/components ssb_dash_components -p package-info.json --r-prefix '' --ignore \\.test\\.",
    "build:backends-activated": "(poetry run npm run build:backends)",
    "build": "npm run build:js && npm run build:backends",
    "build:activated": "npm run build:js && npm run build:backends-activated"
  },
  "author": "Miles Winther <mmw@ssb.no>",
  "license": "MIT",
  "dependencies": {
    "ramda": "^0.30.1",
    "react-docgen": "^5.4.3",
    "react-feather": "^2.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-transform-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@plotly/dash-component-plugins": "^1.2.3",
    "@plotly/webpack-dash-dynamic-import": "^1.3.0",
    "@statisticsnorway/ssb-component-library": "^2.0.97",
    "babel-loader": "^9.1.3",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.70.0",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "styled-jsx": "^5.1.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  }
}

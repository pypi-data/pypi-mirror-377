summary: iSCSI tests using interfaces using bnx2i driver
environment:
  ISCSI_SETUP_VARS: bnx2i
provision+:
  hardware:
    hostname: storageqe-83.lab.eng.brq2.redhat.com
adjust+:
    enabled: false
    when: distro >= rhel-9.5
    because: bnx2 drivers are deprecated in rhel-9.5, and no longer maintained in RHEL-10

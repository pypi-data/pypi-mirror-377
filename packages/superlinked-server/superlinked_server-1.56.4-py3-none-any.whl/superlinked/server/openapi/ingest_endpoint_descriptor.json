{
    "responses": {
        "202": {
            "description": "Accepted"
        },
        "400": {
            "description": "Bad Request",
            "content": {
                "application/json": {
                    "schema": {
                        "type": "object",
                        "properties": {
                            "exception": {
                                "type": "string",
                                "example": "ValueNotProvidedException"
                            },
                            "detail": {
                                "type": "string",
                                "example": "(\"The SchemaField YourSchema.field doesn't have a default value and was not provided in the ParsedSchema.\",)"
                            }
                        },
                        "required": [
                            "exception",
                            "detail"
                        ]
                    }
                }
            }
        }
    }
}

global:
  scrape_interval: 10s
  evaluation_interval: 10s
scrape_configs:
  - job_name: "svc-infra"
    metrics_path: "${metrics_path}"
    static_configs:
      - targets: ["${target}"]
remote_write:
  - url: "${GRAFANA_CLOUD_PROM_URL}"
    basic_auth:
      username: "${GRAFANA_CLOUD_USERNAME}"
      password: "${GRAFANA_CLOUD_TOKEN}"

    "\n",
    "print(\"\\nðŸ—ï¸  HPC Storage Hierarchy Configured:\")\n",
    "print(\"  ðŸ“ Scratch â†’ Work â†’ Campaign â†’ HPSS Archive\")\n",
    "print(\"  ðŸ”„ Analysis Cluster â†” All Storage Tiers\")"

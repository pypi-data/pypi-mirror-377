{# import "./layout.jinja" as Layout #}
{# css "/assets/css/index.css" #}

<Layout>
  {% fill hero %}
  <div class="headline">
    <header>
      <h1>{{ page.title }}</h1>
    </header>
  </div>
  {% endfill %}

  <div class="page page--docs">
    <div class="page__wrapper">
      <div class="page__content">
      {{ page.content }}
      </div>
    </div>
  </div>
</Layout>

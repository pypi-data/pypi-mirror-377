    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug with profile",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Debug z007 Module",
            "type": "debugpy",
            "request": "launch",
            "module": "z007.main",
            "envFile": "${workspaceFolder}/.env",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        }
    ]
}